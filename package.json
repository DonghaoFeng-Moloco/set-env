  "author": "Allen Evans",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "1.5.0",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/npm": "8.0.0",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.9.6",
    "jest": "27.2.1",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.0",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.6",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "4.4.3",
    "webpack": "5.54.0",
    "webpack-cli": "4.8.0"
  }
